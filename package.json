  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "build": "yarn lint && rm -rf ./dist && next build ./src/client && yarn build:types && yarn build:js && yarn copyToDist",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps",
    "build:types": "tsc --project ./tsconfig.json --emitDeclarationOnly",
    "copyToDist": "node ./dist/server/scripts/copyToDist",
    "createIndexes": "node ./dist/server/scripts/createIndexes",
    "dev": "yarn watch & ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=develop NODE_CONFIG_DIR=src/server/config nodemon --watch ./dist ./dist/index.js -p $PORT | ./node_modules/.bin/bunyan",
    "dev:fe": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=develop NODE_CONFIG_DIR=src/server/config node ./dist/index.js -p $PORT | ./node_modules/.bin/bunyan",
    "importExcel": "node ./dist/server/scripts/importExcel",
    "importMetadata": "node ./dist/server/scripts/importMetadata",
    "importNews": "node ./dist/server/scripts/importNews",
    "migrateNewsCount": "node ./dist/server/scripts/migrateNewsCount",
    "lint": "./node_modules/.bin/tslint -c ./tslint.json -p ./tsconfig.json",
    "npm": "yarn build && yarn test && yarn publish",
    "start": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production NODE_CONFIG_DIR=dist/server/config node ./dist/index.js -p $PORT",
    "startB": "yarn build && yarn start",
    "test": "NODE_CONFIG_DIR=src/server/config jest --coverage --verbose",
    "verify": "yarn build",
    "seedDb": "node ./dist/server/scripts/seedDb",
    "watch": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps --watch",
    "provinces": "node ./dist/server/scripts/provinces",
    "updateRoles": "node ./dist/server/scripts/updateRoles",
    "updateFollowCount": "node ./dist/server/scripts/updateFollowCount"
  },
